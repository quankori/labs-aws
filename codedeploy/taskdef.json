{
  "containerDefinitions": [
    {
      "name": "prj-api-ecs-dev",
      "image": "image_url",
      "cpu": 1024,
      "memory": 2048,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "secrets": [
        {
          "name": "NODE_ENV",
          "valueFrom": "arn:aws:ssm:ap-southeast-1:498907343309:parameter/NODE_ENV"
        }
      ],
      "mountPoints": [],
      "volumesFrom": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "prj-api-ecs-logs-dev",
          "awslogs-region": "ap-southeast-1",
          "awslogs-stream-prefix": "ecs_api"
        }
      }
    }
  ],
  "family": "prj-api-ecs-task-def-dev",
  "taskRoleArn": "arn:aws:iam::465257090415:role/prj-api-ecs-role-task-dev",
  "executionRoleArn": "arn:aws:iam::465257090415:role/prj-api-ecs-role-exec-dev",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "2048"
}
